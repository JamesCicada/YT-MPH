{
    "manifest_version": 3,
    "name": "YouTube Homepage History",
    "version": "1.0",
    "permissions": ["storage", "scripting", "activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      },
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/"],
        "js": ["content.js"]
      }
    ]
  }
  